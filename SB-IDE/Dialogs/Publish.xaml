<Window x:Class="SB_IDE.Dialogs.Publish"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Publish" Height="174.8" Width="400" Icon="/SB-IDE;component/AppIcon.ico" ShowInTaskbar="False">
    <Grid>
        <TextBox x:Name="textBoxPublish" Height="23" Margin="10,78,9.6,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Label x:Name="label" Content="Your program was published to the web" Margin="10,10,10,0" VerticalAlignment="Top" HorizontalContentAlignment="Center" FontSize="16" Foreground="#FF1155BB"/>
        <Button x:Name="buttonClose" Content="Close" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="buttonClose_Click"/>
        <Label x:Name="label_Copy" Content="Below is the ID for your program that you can share" Margin="10,47,9.6,0" VerticalAlignment="Top" HorizontalContentAlignment="Center"/>

    </Grid>
</Window>
