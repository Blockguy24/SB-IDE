<Window x:Class="SB_IDE.Dialogs.PolygonSides"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SB_IDE.Dialogs"
        mc:Ignorable="d"
        Title="Polygon Sides" Height="81.6" Width="428.8" Icon="/SB-IDE;component/AppIcon.ico" ShowInTaskbar="False" Topmost="True" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <Label x:Name="label" Content="Enter the number of sides for the polygon (> 3)" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxNumSides" HorizontalAlignment="Left" Height="23" Margin="276,14,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="53" KeyDown="textBoxNumSides_KeyDown"/>
        <Button x:Name="buttonDone" Content="Done" Margin="334,14,10.4,0" VerticalAlignment="Top" Click="buttonDone_Click" Height="22"/>

    </Grid>
</Window>
